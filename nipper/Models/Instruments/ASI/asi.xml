<?xml version="1.0"?>

<PropertyList>

 <path>asi.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>PointerTip</object-name>
  <emission>
   <red>0.5</red>
   <green>0.2</green>
   <blue>0.0</blue>
   <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
  </emission>
 </animation>

 <animation> 
  <type>rotate</type>
  <object-name>PointerCentre</object-name>
  <object-name>PointerTip</object-name>
  <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
  <interpolation>
   <entry><ind>  0</ind><dep>0.0</dep></entry>
   <entry><ind> 48.7</ind><dep>109</dep></entry>
   <entry><ind> 69.5</ind><dep>180</dep></entry>
   <entry><ind> 99.0</ind><dep>242</dep></entry>
   <entry><ind>125</ind><dep>289</dep></entry>
   <entry><ind>152.0</ind><dep>340</dep></entry>
  </interpolation>
  <axis> 
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>  
  </center>
 </animation>         
 
</PropertyList>
